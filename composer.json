{
  "name": "bagrinsergiu/brizy-entities",
  "type": "symfony-bundle",
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/orm": "2.7.6",
    "doctrine/persistence": "^1.3",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^1.8|^2.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/yaml": "^5.2",
    "symfony/validator": "^5.1",
    "trikoder/oauth2-bundle": "^3.2",
    "gedmo/doctrine-extensions": "^3.0.4",
    "bagrinsergiu/base64-encoded-file": "dev-master"
  },

  "autoload": {
    "psr-4": {
      "Brizy\\Bundle\\EntitiesBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Brizy\\Bundle\\EntitiesBundle\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/base64-encoded-file.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/DoctrineExtensions.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/doctrine-orm.git"
    }
  ]
}
