{
  "name": "bagrinsergiu/brizy-entities",
  "type": "symfony-bundle",
  "require": {
    "php": "7.4.*||8.*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/orm": "2.7.6||2.10.3.*",
    "doctrine/persistence": "^2.2",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.3",
    "symfony/framework-bundle": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/validator": "^5.3",
    "trikoder/oauth2-bundle": "^3.2",
    "gedmo/doctrine-extensions": "^3.0.4",
    "nyholm/psr7": "^1.4",
    "hshn/base64-encoded-file": "^3.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.17",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.2.5",
    "symfony/browser-kit": "^5.3",
    "symfony/css-selector": "^5.3",
    "symfony/maker-bundle": "^1.14",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/profiler-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Brizy\\Bundle\\EntitiesBundle\\": "src/",
      "Brizy\\Bundle\\EntitiesBundle\\TestFactory\\": "tests/Factory/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Brizy\\Bundle\\EntitiesBundle\\Tests\\": "tests/"
    },
    "exclude-from-classmap": [
      "/Factory/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/DoctrineExtensions.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/bagrinsergiu/doctrine-orm.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/alecszaharia/pre-commit-php-cs.git"
    }
  ]
}
